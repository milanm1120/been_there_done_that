<h1> Edit Destinations Form</h1>
<%= form_with(model: @destination) do |f| %>
    <%= f.label :city %>
    <%= f.text_field :city %>
    <%= f.label :state %>
    <%= f.text_field :state %>
    <%= f.label :country %>
    <%= f.text_field :country %>
    <%= submit_tag "Update Destination" %>
<% end %>